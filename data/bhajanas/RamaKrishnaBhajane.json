    "en": {
      "title": "Sri Rama Krishna Bhajane",
      "content": [
        {
          "type": "paragraph",
          "lines": ["Rama Namo Rama Namo Krishna Namo Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Ayodhya Vaasi Rama Namo", "Gokula Vaasi Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": [
            "Dasharatha Nandana Rama Namo",
            "Vasudeva Nandana Krishna Namo"
          ]
        },
        {
          "type": "paragraph",
          "lines": ["Kausalya Nandana Rama Namo", "Devaki Nandana Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Ahalyoddharaka Rama Namo", "Kubjoddharaka Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Taataki Naashana Rama Namo", "Putani Sudana Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Vaali Mardana Rama Namo", "Kaali Nartana Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Sugriva Priya Rama Namo", "Partha Priya Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Anjani Varada Rama Namo", "Akrura Varada Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Ravana Naashaka Rama Namo", "Kamsa Dvamsaka Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Seeta Kanta Rama Namo", "Rukmini Vallabha Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Bhakta Vatsala Rama Namo", "Bhakta Priya Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Ananda Murti Rama Namo", "Gaana Vinoda Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Anaatha Rakshaka Rama Namo", "Apadbandhava Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": ["Dharma Murti Rama Namo", "Dharma Paalaka Krishna Namo"]
        },
        {
          "type": "paragraph",
          "lines": [
            "Jaya Jaya Jaya Jaya Rama Namo",
            "Jaya Jaya Jaya Jaya Krishna Namo"
          ]
        }
