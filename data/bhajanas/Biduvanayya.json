    "en": {
      "title": "Hanuma Biduvenenayya",
      "content": [
        {
          "type": "paragraph",
          "lines": [
            "Biduvenenayya Hanuma Biduvenenayya | ",
            "Biduveneno Hanuma Ninna |",
            "Adigalige Shirava Baagi |",
            "Jnana Bhakti Vairagyavanu |",
            "Koduva Tanaka Summane Naanu ||P||"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Hastavanu Ettidareenu |",
            "Haaraakaala Haakalenu |",
            "Bhrutyanu Ninnavanu Naanu |",
            "Atti Varadana Toruva Tanaka ||1|| "
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Hallumudiya Kachidareenu  |",
            "Anjuveneno Ninage Naanu |",
            "Pullanaabhavalli Manassu |",
            "Nilliso Tanaka Summane Naanu ||2||"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Donkumoore Baalava Tiddi  |",
            "Hoonkarisidare Anjuvenalla |",
            "Kinkara Ninage Kinkara Naanu |",
            "Purandara Vittalana Toruva Tanaka ||3||"
          ]
        }
      ]
