    "en":{
      "title": "Sri Ramachandirane Sri Lola Sundarane",
      "content": [
        {
          "type": "paragraph",
          "lines": [
            "Sri Ramachandirane Sri Lola Sundarane",
            "Srimannarayana Ram Ram Ram"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Dasharatha puttirane Dasharamittirane",
            "Kaataka samhaaranane Ram Ram Ram ||1||"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Vaikunthavaasane Vanavihaarane",
            "Sheshaadri shayanane Ram Ram Ram ||2||"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Paalane Paripaalane Gokula Sundarane Gopikumaarane",
            "Govinda ramane Ram Ram Ram ||3||"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Saara suguna shareerane, Ramaabhiramane Raghu kula somane",
            "Janakiya vallabhane Ram Ram Ram ||4||"
          ]
        }
      ]
    }
