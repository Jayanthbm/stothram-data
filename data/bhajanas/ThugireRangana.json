    "en": {
      "title": "Toogire Rangana Toogire Krishnana",
      "content": [
        {
          "type": "paragraph",
          "lines": [
            "Toogire Rangana Toogire Krishnana",
            "Toogire Achyutanantana",
            "Toogire Varagiriappa Timmappana",
            "Toogire Kaveri Rangayyana"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Naagalokadalli Narayana Malagyane",
            "Naaga Kannikeyaru Toogire",
            "Naagaveniyaru Nena Pididukondu",
            "Begane Tottile Toogire ||1||"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Indralokadallupendra Malagyane",
            "Indumukhiyarella Toogire",
            "Indra Kannikeyaru Chandadi Bandu",
            "Mukundana Tottile Toogire ||2||"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Aaladyeleya Mele Shreelola Malagyane",
            "Neelakuntaleyaru Toogire",
            "Vyaalashayana Hari Malagu Malagendu",
            "Baala Krishnayyana Toogire ||3||"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Saasira Naamane Sarvottamane Endu",
            "Soosutta Tottile Toogire",
            "Vale Saagi Maduvinol Sheshana Tuliditta",
            "Dosha Vidoorana Toogire ||4||"
          ]
        },
        {
          "type": "paragraph",
          "lines": [
            "Arale Le Magaayi Koral Muttina Haara",
            "Taralana Tottile Toogire",
            "Siridevi Ramanane Purandara Vittalane",
            "Karunadi Malagendu Toogire ||5||"
          ]
        }
      ]
    }
